<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 <configuration>
 	<typeAliases>
      <typeAlias type="member.model.Member" alias="MyMember"/>
       <typeAlias type="seller.model.Seller" alias="MySeller"/>
       <typeAlias type="hotel.model.Hotel" alias="MyHotel"/>
       <typeAlias type="admin.model.Admin" alias="MyAdmin"/>
       <typeAlias type="hotel.model.Room" alias="MyRoom"/>
       <typeAlias type="order.model.Order" alias="MyOrder"/>
    </typeAliases>
      
   <mappers>
      <mapper resource="mybatis/member.xml"></mapper>
      <mapper resource="mybatis/seller.xml"></mapper>
      <mapper resource="mybatis/admin.xml"></mapper>
      <mapper resource="mybatis/hotel.xml"></mapper>
      <mapper resource="mybatis/room.xml"/>
      <mapper resource="mybatis/order.xml"/>
   </mappers>
 
 </configuration>
